            "get": {
                "description": "Возвращает ближайшие точки интереса по координатам",
                "tags": [
                    "POI"
                ],
                "summary": "Поиск ближайшей точки интереса",
                "parameters": [
                    {
                        "type": "number",
                        "example": 55.7558,
                        "description": "Широта",
                        "name": "latitude",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "number",
                        "example": 37.6173,
                        "description": "Долгота",
                        "name": "longitude",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/domain.PointOfInterest"
                        }
                    }
                }
            }
        },
